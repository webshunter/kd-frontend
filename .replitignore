# Files to ignore in Replit
# This file tells Replit which files to ignore when syncing

# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Build outputs
dist/
dist-ssr/
*.local

# Environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db
ehthumbs.db
Desktop.ini

# Logs
logs/
*.log

# Temporary files
tmp/
temp/
.tmp/

# Git files (optional - Replit handles git differently)
.git/
.gitignore

# Coverage reports
coverage/
.nyc_output/

# Cache directories
.cache/
.parcel-cache/

# TypeScript build info
*.tsbuildinfo

# ESLint cache
.eslintcache

# Optional npm cache
.npm

# Storybook
.out
.storybook-out

# Next.js
.next

# Nuxt.js
.nuxt

# Gatsby
public

