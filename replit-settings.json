{
  "name": "KD Frontend - Kampung Digital Tangsel",
  "description": "Frontend untuk platform UMKM Kampung Digital Tangerang Selatan",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "dev": "npm run dev",
    "build": "npm run build",
    "preview": "npm run preview",
    "workflow": "./workflow.sh",
    "start": "./workflow.sh"
  },
  "keywords": [
    "umkm",
    "tangerang-selatan",
    "frontend",
    "react",
    "vite",
    "replit"
  ],
  "author": "Kampung Digital Team",
  "license": "MIT",
  "replit": {
    "language": "nodejs",
    "nix": "nixpkgs",
    "run": "./workflow.sh",
    "port": 5000,
    "alwaysRun": true,
    "openFiles": [
      "index.html",
      "package.json",
      "vite.config.ts"
    ],
    "hidden": [
      ".replit",
      "replit.nix",
      ".replitignore",
      "workflow.sh",
      "start.sh",
      "history.txt"
    ],
    "env": {
      "NODE_ENV": "development",
      "VITE_BACKEND_URL": "https://kdtangsel.hubunk.id",
      "VITE_API_URL": "/api",
      "VITE_FRONTEND_URL": "http://localhost:5000",
      "VITE_ALLOWED_HOSTS": "tangsel.hubunk.id,kdtangsel.hubunk.id",
      "CI": "false",
      "DISABLE_ESLINT_PLUGIN": "true"
    },
    "build": {
      "command": "npm run build",
      "outputDir": "dist",
      "install": "npm install",
      "start": "npm run dev",
      "preview": "npm run preview"
    },
    "debugger": {
      "support": true,
      "transport": "localhost:0",
      "startCommand": "npm run dev"
    },
    "lsp": {
      "language": "typescript"
    },
    "fileTypes": {
      "*.tsx": "typescript",
      "*.ts": "typescript",
      "*.jsx": "javascript",
      "*.js": "javascript"
    },
    "git": {
      "autoCommit": false,
      "alwaysPull": false,
      "alwaysPush": false,
      "ignore": [
        "node_modules/",
        "dist/",
        ".git/",
        "*.log",
        ".env.local",
        ".env.production"
      ]
    }
  },
  "features": {
    "autoPull": true,
    "autoInstall": true,
    "autoStart": true,
    "hotReload": true,
    "portFallback": true,
    "securityAudit": true,
    "buildTest": true,
    "environmentSetup": true
  },
  "requirements": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0",
    "memory": ">=512MB",
    "storage": ">=100MB"
  },
  "deployment": {
    "platform": "replit",
    "region": "auto",
    "scaling": "manual",
    "domains": ["auto-generated"]
  }
}
